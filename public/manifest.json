{
  "manifest_version": 3,
  "name": "qriosity",
  "version": "1.1",
  "description": "Generate QRcode from links withou ads ðŸ˜Š",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "clipboardRead"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}